{
    "name" : "sonatra/form-extensions-bundle",
    "type": "symfony-bundle",
    "description": "Add new form types for Symfony2",
    "keywords": ["datetime", "date", "time", "picker", "form", "hammerjs", "knob", "select2", "ajax"],
    "homepage": "https://github.com/sonatra/SonatraFormExtensionsBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "FranÃ§ois Pluchino",
            "email": "francois.pluchino@sonatra.com"
        }
    ],
    "require" : {
        "php" : ">=5.5.0",
        "symfony/framework-bundle" : "~3.0",
        "fxp/require-asset" : "1.0.*@dev",
        "fxp/require-asset-bundle" : "1.0.*@dev",
        "sonatra/ajax-bundle" : "1.0.*@dev",
        "symfony/serializer": "~3.0",
        "symfony/form": "~3.0"
    },
    "require-dev" : {
        "symfony/doctrine-bridge" : "~3.0",
        "doctrine/orm" : "~2.5"
    },
    "suggest": {
        "fxp/composer-asset-plugin": "For manage NPM/Bower dependencies with Composer",
        "bower-asset/select2": "The Select2 jquery plugin",
        "bower-asset/sonatra-jquery-datetime-picker": "The datetime picker jquery plugin"
    },
    "autoload": {
        "psr-4": { "Sonatra\\Bundle\\FormExtensionsBundle\\": "" },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "extra" : {
        "branch-alias" : {
            "dev-master" : "1.0-dev"
        }
    }
}
